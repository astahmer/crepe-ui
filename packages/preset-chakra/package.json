{
  "name": "@crepe-ui/preset-chakra",
  "version": "0.0.1",
  "types": "./src/preset.ts",
  "main": "./dist/preset.js",
  "exports": {
    ".": {
      "source": "./src/preset.ts",
      "types": "./dist/preset.d.ts",
      "import": "./dist/preset.mjs",
      "require": "./dist/preset.js"
    },
    "./vars": {
      "source": "./src/vars.ts",
      "types": "./dist/vars.d.ts",
      "import": {
        "types": "./dist/vars.d.mts",
        "default": "./dist/vars.mjs"
      },
      "require": "./dist/vars.js"
    },
    "./reset.css": "./reset.css"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "dependencies": {
    "@ark-ui/anatomy": "^0.1.0",
    "@crepe-ui/shared": "workspace:^",
    "@pandacss/dev": "^0.24.1"
  },
  "devDependencies": {
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/astahmer/crepe-ui",
    "directory": "packages/preset-chakra"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ]
